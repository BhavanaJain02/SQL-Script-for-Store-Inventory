	"SALES_ORDER_ID" NUMBER, 
	"PRODUCT_ID" NUMBER, 
	"QUANTITY" NUMBER, 
	"PRICE" NUMBER, 
	 PRIMARY KEY ("SALES_ORDER_D_ID")
  USING INDEX  ENABLE
   ) ;

  CREATE TABLE "SM_REACTIONS" 
   (	"ID" NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"POST_ID" NUMBER, 
	"REACTION" VARCHAR2(16 CHAR), 
	"LAT" NUMBER, 
	"LON" NUMBER, 
	"CREATED" DATE NOT NULL ENABLE, 
	"CREATED_BY" VARCHAR2(255 CHAR) NOT NULL ENABLE, 
	"UPDATED" DATE NOT NULL ENABLE, 
	"UPDATED_BY" VARCHAR2(255 CHAR) NOT NULL ENABLE, 
	 CONSTRAINT "SM_REACTIONS_ID_PK" PRIMARY KEY ("ID")
  USING INDEX  ENABLE, 
	 CONSTRAINT "SM_REACTIONS_CON" UNIQUE ("POST_ID", "CREATED_BY")
  USING INDEX  ENABLE
   ) ;

  CREATE TABLE "SUPPLIER" 
