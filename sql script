	"CONFIRMATION" VARCHAR2(255), 
	"SHIPPER" VARCHAR2(50), 
	 PRIMARY KEY ("ID")
  USING INDEX  ENABLE
   ) ;

  CREATE TABLE "SALES_ORDER" 
   (	"SALES_ORDER_ID" NUMBER, 
	"SALES_ORDER_NO" VARCHAR2(20), 
	"SUPPLIER_ID" NUMBER, 
	"ORDER_DATE" DATE, 
	"ORDER_STATUS" VARCHAR2(20), 
	"TOTAL_PRICE" NUMBER, 
	 PRIMARY KEY ("SALES_ORDER_ID")
  USING INDEX  ENABLE
   ) ;

  CREATE TABLE "SALES_ORDER_D" 
